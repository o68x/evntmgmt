<h1>Look, you're an admin now!</h1>

<ul>
<% @users.each do |user| %>
    <li>
        <%= link_to user.email, admin_user_path(user.id) %>
        <%= link_to "modifier", edit_admin_user_path(user.id) %>
        <%= link_to "supprimer", admin_user_path(user.id), method: :delete %>
    </li>
<% end %>
</ul>
